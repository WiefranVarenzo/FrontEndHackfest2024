<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:background="#FFFFFF">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="650dp"
        android:background="@drawable/maps_pagee">

        <View
            android:layout_width="250dp"
            android:layout_height="58dp"
            android:layout_marginStart="90dp"
            android:layout_marginTop="190dp"
            android:background="@drawable/green_maps" />
        <View
            android:layout_width="250dp"
            android:layout_height="58dp"
            android:layout_marginStart="190dp"
            android:layout_marginTop="50dp"
            android:background="@drawable/green_maps" />
        <View
            android:layout_width="250dp"
            android:layout_height="58dp"
            android:layout_marginStart="-40dp"
            android:layout_marginTop="460dp"
            android:background="@drawable/green_maps" />
        <View
            android:layout_width="250dp"
            android:layout_height="58dp"
            android:layout_marginStart="120dp"
            android:layout_marginTop="570dp"
            android:background="@drawable/green_maps" />
        <View
            android:layout_width="250dp"
            android:layout_height="58dp"
            android:layout_marginStart="1dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/green_maps" />
        <View
            android:layout_width="205dp"
            android:layout_height="58dp"
            android:layout_marginStart="160dp"
            android:layout_marginTop="490dp"
            android:background="@drawable/yellow_maps"/>
        <View
            android:layout_width="205dp"
            android:layout_height="58dp"
            android:layout_marginStart="-50dp"
            android:layout_marginTop="120dp"
            android:background="@drawable/yellow_maps"/>
        <View
            android:layout_width="205dp"
            android:layout_height="58dp"
            android:layout_marginStart="90dp"
            android:layout_marginTop="340dp"
            android:background="@drawable/yellow_maps"/>
        <View
            android:layout_width="205dp"
            android:layout_height="58dp"
            android:layout_marginStart="-70dp"
            android:layout_marginTop="580dp"
            android:background="@drawable/yellow_maps"/>
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="650dp"
        android:orientation="horizontal"
        android:layout_gravity="bottom">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_marginTop="10dp">
        </TextView>
        <View
            android:layout_width="match_parent"
            android:visibility="visible"
            android:layout_height="1.5dp"
            android:layout_marginTop="0dp"
            android:background="#808080"/>

        <Button
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginStart="25dp"
            android:layout_marginTop="15dp"
            android:backgroundTint="#ffffff"
            tools:ignore="SpeakableTextPresentCheck"></Button>

        <Button
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginStart="97dp"
            android:layout_marginTop="15dp"
            android:backgroundTint="#ffffff"
            tools:ignore="SpeakableTextPresentCheck"></Button>

        <Button
            android:layout_width="80dp"
            android:layout_height="70dp"
            android:layout_marginStart="174dp"
            android:layout_marginTop="15dp"
            android:backgroundTint="#ffffff"
            tools:ignore="SpeakableTextPresentCheck"></Button>

        <Button
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginStart="275dp"
            android:layout_marginTop="15dp"
            android:backgroundTint="#ffffff"
            tools:ignore="SpeakableTextPresentCheck"></Button>

        <Button
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginStart="335dp"
            android:layout_marginTop="15dp"
            android:backgroundTint="#ffffff"
            tools:ignore="SpeakableTextPresentCheck"></Button>
        <View
            android:id="@+id/HomePage"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginStart="35dp"
            android:layout_marginTop="25dp"
            android:backgroundTint="@color/black"
            android:background="@drawable/home"/>
        <View
            android:id="@+id/ExplorePage"
            android:layout_width="55dp"
            android:layout_height="55dp"
            android:layout_marginStart="95dp"
            android:layout_marginTop="12dp"
            android:background="@drawable/social_btn"/>
        <View
            android:id="@+id/MapPage"
            android:layout_width="85dp"
            android:layout_height="100dp"
            android:layout_marginStart="172dp"
            android:layout_marginTop="1.5dp"
            android:background="@drawable/rounded_corner_map"/>
        <View
            android:id="@+id/MessagePage"
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:layout_marginStart="191dp"
            android:layout_marginTop="19dp"
            android:background="@drawable/maps_ic"/>
        <View
            android:id="@+id/ProfilPage"
            android:layout_width="55dp"
            android:layout_height="55dp"
            android:layout_marginStart="275dp"
            android:layout_marginTop="11dp"
            android:background="@drawable/chat_btn"/>
        <View
            android:layout_width="55dp"
            android:layout_height="55dp"
            android:layout_marginStart="335dp"
            android:layout_marginTop="11dp"
            android:background="@drawable/profil_btn"/>
    </RelativeLayout>

</RelativeLayout>